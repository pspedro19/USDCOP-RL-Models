        {
          "datasource": {
            "type": "postgres",
            "uid": "timescaledb"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH trade_pnl AS (\n  SELECT \n    CASE WHEN exit_price > entry_price AND action_type = 'LONG' THEN exit_price - entry_price\n         WHEN exit_price < entry_price AND action_type = 'SHORT' THEN entry_price - exit_price\n         WHEN exit_price < entry_price AND action_type = 'LONG' THEN exit_price - entry_price\n         WHEN exit_price > entry_price AND action_type = 'SHORT' THEN entry_price - exit_price\n         ELSE 0 END as pnl\n  FROM agent_actions\n  WHERE timestamp >= $__timeFrom() AND timestamp <= $__timeTo()\n    AND exit_price IS NOT NULL\n)\nSELECT \n  CASE \n    WHEN ABS(AVG(CASE WHEN pnl < 0 THEN pnl END)) > 0 \n    THEN ABS(AVG(CASE WHEN pnl > 0 THEN pnl END) / AVG(CASE WHEN pnl < 0 THEN pnl END))\n    ELSE 0 \n  END as profit_factor\nFROM trade_pnl",
          "refId": "A"
        }
      ],
      "title": "Profit Factor",
      "type": "stat"
    },
