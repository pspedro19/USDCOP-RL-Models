{
  "model_name": "Model B (Aggressive)",
  "algorithm": "PPO (Proximal Policy Optimization)",
  "framework": "stable-baselines3",
  "version": "V19",
  "validated_date": "2025-12-26",

  "hyperparameters": {
    "learning_rate": 1e-4,
    "n_steps": 2048,
    "batch_size": 128,
    "n_epochs": 10,
    "gamma": 0.99,
    "ent_coef": 0.05,
    "clip_range": 0.2,
    "gae_lambda": 0.95,
    "max_grad_norm": 0.5,
    "vf_coef": 0.5
  },

  "policy": {
    "type": "MlpPolicy",
    "net_arch": [256, 256],
    "activation_fn": "Tanh"
  },

  "environment": {
    "class": "TradingEnvironmentV19",
    "initial_balance": 10000,
    "max_position": 1.0,
    "episode_length": 400,
    "use_vol_scaling": true,
    "use_regime_detection": true,
    "bars_per_day": 56
  },

  "training": {
    "total_timesteps": 80000,
    "seed": 42,
    "device": "cpu"
  },

  "validation_results": {
    "stress_tests": {
      "passed": true,
      "pass_rate": 0.60,
      "periods_tested": 5,
      "periods_passed": 3
    },
    "five_fold_cv": {
      "passed": true,
      "mean_sharpe": 2.21,
      "std_sharpe": 2.01,
      "mean_max_dd": 0.002,
      "positive_sharpe_folds": 4
    }
  },

  "risk_limits": {
    "max_drawdown_threshold": 0.05,
    "max_hold_percentage": 0.90,
    "min_sharpe_30day": 0.0
  },

  "data": {
    "source": "RL_DS3_MACRO_CORE.csv",
    "timeframe": "5min",
    "features": 21,
    "date_range": {
      "start": "2020-03-02",
      "end": "2025-12-05"
    }
  }
}
