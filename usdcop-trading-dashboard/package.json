    "test:integration": "vitest run tests/integration",
    "test:all": "npm run test:run && npm run test:e2e && npm run test:accessibility",
    "playwright:install": "playwright install",
    "lighthouse": "lighthouse-ci",
    "lighthouse:mobile": "lighthouse-ci --preset=mobile",
    "quality:check": "npm run lint && npm run test:coverage && npm run test:e2e",
    "quality:report": "npm run test:coverage && npm run lighthouse && npm run test:accessibility",
    "reports:generate": "node tests/reports/generate-report.js",
    "reports:serve": "npx http-server tests/reports -p 8080 -o"
