        "targets": [
          {
            "expr": "l0_pipeline_status",
            "refId": "A",
            "format": "time_series",
            "legendFormat": "Pipeline Status"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Failed",
                    "color": "red"
                  },
                  "1": {
                    "text": "Running",
                    "color": "green"
                  },
                  "2": {
                    "text": "Completed",
                    "color": "blue"
                  },
                  "3": {
                    "text": "Idle",
                    "color": "yellow"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
