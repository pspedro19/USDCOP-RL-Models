{
  "dashboard": {
    "id": null,
    "title": "USD/COP Pipeline Health Overview",
    "tags": ["usdcop", "trading", "pipeline", "health", "overview"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "layer",
          "type": "custom",
          "options": [
            {
              "text": "All",
              "value": "$__all",
              "selected": true
            },
            {
              "text": "L0 - Raw Data",
              "value": "l0",
              "selected": false
            },
            {
              "text": "L1 - Features",
              "value": "l1",
              "selected": false
            },
            {
              "text": "L2 - Quality",
              "value": "l2",
              "selected": false
            },
            {
              "text": "L3 - Correlations",
              "value": "l3",
              "selected": false
            },
            {
              "text": "L4 - RL Ready",
              "value": "l4",
              "selected": false
            },
            {
              "text": "L5 - Serving",
              "value": "l5",
              "selected": false
            }
          ],
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Pipeline Components Health",
        "type": "heatmap",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "pipeline_component_health{layer=~\"$layer\"}",
            "refId": "A",
            "legendFormat": "{{layer}}"
          }
        ],
        "options": {
          "calculate": false,
          "calculation": {},
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "reverse": false,
            "scale": "exponential",
            "scheme": "RdYlGn",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": false
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Data Flow Rate",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(pipeline_records_processed_total{layer=~\"$layer\"}[5m])",
            "refId": "A",
            "legendFormat": "{{layer}} - Records/sec"
          }
        ],
        "options": {
          "legend": {
            "calcs": ["last", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Records/sec",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "hue",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate by Component",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(pipeline_errors_total{layer=~\"$layer\"}[5m])",
            "refId": "A",
            "legendFormat": "{{layer}} - Errors/sec"
          }
        ],
        "options": {
          "legend": {
            "calcs": ["last", "max"],
            "displayMode": "table",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Errors/sec",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 20,
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "auto",
              "spanNulls": false
            },
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Backup Status",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "backup_health_score{layer=~\"$layer\"}",
            "refId": "A",
            "legendFormat": "{{layer}}"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Error",
                    "color": "red"
                  },
                  "1": {
                    "text": "Warning",
                    "color": "yellow"
                  },
                  "2": {
                    "text": "Good",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 2
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "API Usage %",
        "type": "gauge",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "targets": [
          {
            "expr": "api_usage_percentage",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 6,
        "title": "WebSocket Clients",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "websocket_connected_clients",
            "refId": "A",
            "legendFormat": "Connected"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 5
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Active Alerts",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(active_alerts)",
            "refId": "A",
            "legendFormat": "Total Alerts"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Data Latency Distribution",
        "type": "histogram",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, pipeline_latency_seconds{layer=~\"$layer\"})",
            "refId": "A",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, pipeline_latency_seconds{layer=~\"$layer\"})",
            "refId": "B",
            "legendFormat": "50th percentile"
          }
        ],
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Latency (seconds)",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "Resource Utilization",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "pipeline_cpu_usage_percent{layer=~\"$layer\"}",
            "refId": "A",
            "legendFormat": "{{layer}} - CPU %"
          },
          {
            "expr": "pipeline_memory_usage_percent{layer=~\"$layer\"}",
            "refId": "B",
            "legendFormat": "{{layer}} - Memory %"
          }
        ],
        "options": {
          "legend": {
            "calcs": ["last", "max"],
            "displayMode": "table",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Usage %",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineInterpolation": "smooth",
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(pipeline_restart_total[1h]) > 0",
          "iconColor": "red",
          "name": "Pipeline Restarts",
          "showIn": 0,
          "step": "60s",
          "tagKeys": "layer",
          "titleFormat": "Pipeline Restart",
          "type": "tags",
          "useValueForTime": false
        }
      ]
    }
